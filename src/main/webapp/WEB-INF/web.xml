<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>MajorProjectServlet</servlet-name>
        <servlet-class>MajorProjectServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Mind_map_servlet</servlet-name>
        <servlet-class>Mind_map_servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Offline_Summary</servlet-name>
        <servlet-class>Offline_Summary</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MajorProjectServlet</servlet-name>
        <url-pattern>/MajorProjectServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Mind_map_servlet</servlet-name>
        <url-pattern>/Mind_map_servlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Offline_Summary</servlet-name>
        <url-pattern>/Offline_Summary</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
